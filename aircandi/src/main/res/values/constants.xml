<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- Names -->

	<string name="name_app_family">Aircandi</string>
	<string name="name_app_family_lowercase">aircandi</string>

	<string name="name_app">Aircandi</string>
	<string name="name_app_lowercase">aircandi</string>
	<string name="name_company">3meters LLC.</string>

	<string name="app_family_attribution">by [@string/name_app_family_lowercase]</string>

	<!--
		We get complete substitution with the following exceptions
		- strings for menu titles.
		- strings for activity titles in manifest.
		- strings in array resources.
	 -->

	<string name="container_singular">Place</string>
	<string name="container_plural">Places</string>
	<string name="container_singular_lowercase">place</string>
	<string name="container_plural_lowercase">places</string>

	<!-- Identifiers -->

	<string name="id_gcm_sender">657673071389</string>
	<string name="id_package">com.aircandi</string>
	<string name="id_referrer">com.aircandi</string>

	<!-- Late binding: Aircandi applications always subclass GcmIntentService -->

	<string name="class_gcm_intent_service">[@string/id_package].components.GcmIntentService</string>

	<!-- Overrides strings in pull to refresh library -->

	<!-- Splash -->

	<string name="splash_message">get more out of the places you love</string>
	<string name="splash_button_signin">Sign in</string>
	<string name="splash_button_signup">Register</string>
	<string name="splash_button_start">Ghost</string>
	<string name="splash_button_retry">Retry</string>

	<!-- Invite -->

	<string name="invite_heading">Invite your friends to [@string/name_app].</string>
	<string name="invite_message_default">I enjoy [@string/name_app] and I thought you might like it too!</string>
	<string name="form_invite_email_hint">Email address</string>
	<string name="form_invite_message_hint">Add a personal note (optional)</string>

	<!-- Feedback -->

	<string name="feedback_message">How can we make [@string/name_app] better for you?</string>

	<!-- Runtime settings -->

	<bool name="entity_fencing_enabled">false</bool>

	<!-- Content links -->

	<string name="url_terms">http://candipatch.com/terms</string>
	<string name="url_privacy">http://candipatch.com/privacy</string>
	<string name="url_legal">http://candipatch.com/android</string>

	<string name="s3_bucket_images">aircandi-images</string>
	<string name="s3_bucket_thumbnails">aircandi-thumbnails</string>
	<string name="s3_bucket_users">aircandi-users</string>
	<string name="url_media_images">http://[@string/s3_bucket_images].s3.amazonaws.com/</string>
	<string name="url_media_thumbnails">http://[@string/s3_bucket_thumbnails].s3.amazonaws.com/</string>
	<string name="url_media_users">http://[@string/s3_bucket_users].s3.amazonaws.com/</string>

	<string name="uri_app_update">market://details?id=[@string/id_package]&amp;referrer=utm_source%3D[@string/id_referrer]</string>
	<string name="uri_app_update_web">http://play.google.com/store/apps/details?id=[@string/id_package]&amp;referrer=utm_source%3D[@string/id_referrer]</string>

	<!-- Service paging sizes -->

	<integer name="page_size_entities">30</integer>
	<integer name="page_size_applinks">100</integer>
	<integer name="page_size_comments">30</integer>
	<integer name="page_size_pictures">30</integer>
	<integer name="page_size_users">30</integer>
	<integer name="page_size_activities">30</integer>

</resources>