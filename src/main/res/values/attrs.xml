<?xml version="1.0" encoding="utf-8"?>
<resources>

	<!-- List Pref Multi Select -->

	<attr name="keyId" format="integer" />
	<attr name="separator" format="string" />
	<attr name="checkAll" format="string" />

	<!-- Images -->

	<attr name="showBusy" format="boolean" />
	<attr name="shape" format="string" />
	<attr name="radius" format="integer" />
	<attr name="aspectRatio" format="float" />
	<attr name="category" format="enum">
		<enum name="none" value="0" />
		<enum name="standard" value="1" />
		<enum name="thumbnail" value="2" />
		<enum name="profile" value="3" />
	</attr>
	<attr name="config" format="enum">
		<enum name="rgb_565" value="1" />
		<enum name="argb_8888" value="3" />
	</attr>

	<!-- Theme attributes -->

	<declare-styleable name="AppTheme">

		<attr name="textAppearanceMediumLarge" format="reference" />
