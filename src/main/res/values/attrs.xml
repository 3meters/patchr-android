<?xml version="1.0" encoding="utf-8"?>
<resources>

	<attr name="layoutId" format="integer" />
	<attr name="layoutRef" format="reference" />
	<attr name="textId" format="integer" />
	<attr name="keyId" format="integer" />
	<attr name="label" format="integer|string" />
	<attr name="maxWidth" format="dimension" />
	<attr name="drawable" format="reference" />
	<attr name="drawableScale" format="float" />

	<attr name="parallax_factor" format="float" />
	<attr name="inner_parallax_factor" format="float" />
	<attr name="parallax_views_num" format="integer" />
	<attr name="circular_parallax" format="boolean" />
	<attr name="layoutHeader" format="integer" />
	<attr name="layoutFooter" format="integer" />
	<attr name="headerTitle" format="string" />
	<attr name="layoutItems" format="integer" />
	<attr name="itemMaxCount" format="integer" />
