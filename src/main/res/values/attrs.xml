	<attr name="checkAll" format="string" />
	<attr name="separator" format="string" />
	<attr name="prefix" format="string" />
	<attr name="showBusy" format="boolean" />
	<attr name="transformKey" format="string" />

	<!-- Images -->

	<attr name="aspectRatio" format="float" />
	<attr name="aspectRatioEnabled" format="boolean" />
	<attr name="dominantMeasurement">
		<enum name="width" value="0" />
		<enum name="height" value="1" />
	</attr>
	<attr name="fitType" format="enum">
		<enum name="auto" value="0" />
		<enum name="fixed" value="1" />
		<enum name="none" value="2" />
	</attr>
	<attr name="sizeCategory" format="enum">
		<enum name="none" value="0" />
		<enum name="standard" value="1" />
		<enum name="thumbnail" value="2" />
		<enum name="profile" value="3" />
	</attr>
	<attr name="offsetWidth" format="float" />
	<attr name="offsetHeight" format="float" />
	<attr name="sizeHint" format="dimension" />
	<attr name="config" format="enum">
		<enum name="rgb_565" value="1" />
		<enum name="argb_8888" value="3" />
	</attr>

